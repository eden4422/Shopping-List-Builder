<Window x:Class="Shopping_list_builder.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shopping_list_builder"
        mc:Ignorable="d"
        Title="Input Dialog" Height="150" Width="250">
    <Grid>
        <TextBox x:Name="textBox" Margin="10" VerticalAlignment="Center"/>
        <Button Content="Enter" Margin="0,100,0,10" HorizontalAlignment="Center" Click="EnterButton_Click" Width="115"/>
    </Grid>
</Window>
